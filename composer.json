{
    "name": "basketin/cart",
    "description": "Cart for eCommerce store",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Karim Mohamed",
            "email": "komicho1996@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Obelaw\\Basketin\\Cart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Obelaw\\Basketin\\Cart\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1||^8.2",
        "laravel/framework": "^10.0||^11.0||^12.0",
        "filament/filament": "^3.2||^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.16",
        "pestphp/pest-plugin-laravel": "*",
        "laravel/legacy-factories": "^1.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Obelaw\\Basketin\\Cart\\Providers\\BasketinCartServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    }
}
