{
    "name": "storephp/cart",
    "description": "Cart for eCommerce store",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Karim Mohamed",
            "email": "komicho1996@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Storephp\\Cart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Storephp\\Cart\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.16",
        "pestphp/pest-plugin-laravel": "*",
        "laravel/legacy-factories": "^1.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Storephp\\Cart\\Providers\\StorePHPCartServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    }
}
